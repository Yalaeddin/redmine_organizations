<h2><%= link_to l(:label_organization_plural), organizations_path %> &#187; <%= l(:label_organization_new) %></h2>

<%= labelled_form_for :organization, @organization, :url => {:controller => 'organizations', :action => 'create'}, :html => {:class => nil} do |f| -%>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <p>
    <%= submit_tag l(:button_create) %>
    <%= link_to_function l('cancel'), "history.back()" %>
  </p>
<% end -%>

